{
	"name": "chessalyzer.js",
	"version": "1.0.1",
	"description": "Batch analyze PGN and generate heat maps",
	"main": "lib/chessalyzer.js",
	"scripts": {
		"build": "webpack --env dev && webpack --env build",
		"dev": "webpack --progress --colors --watch --env dev",
		"test": "mocha --require babel-register --colors ./test/*.spec.js",
		"test:watch": "mocha --require babel-register --colors -w ./test/*.spec.js",
		"jsdoc": "./node_modules/.bin/jsdoc -t ./node_modules/ink-docstrap/template src -d docs"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/PeterPain/chessalyzer.js"
	},
	"keywords": [
		"chess",
		"pgn",
		"heatmap",
		"batch",
		"analysis"
	],
	"author": "Yannik Schr√∂der",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/PeterPain/chessalyzer.js/issues"
	},
	"homepage": "https://github.com/PeterPain/chessalyzer.js",
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.4.4",
		"@babel/preset-env": "^7.4.4",
		"@babel/register": "^7.4.4",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.6",
		"babel-plugin-add-module-exports": "^1.0.2",
		"chai": "^4.2.0",
		"eslint": "^5.16.0",
		"eslint-config-airbnb": "^17.1.0",
		"eslint-loader": "^2.1.2",
		"eslint-plugin-import": "^2.17.2",
		"eslint-plugin-jsx-a11y": "^6.2.1",
		"eslint-plugin-react": "^7.13.0",
		"ink-docstrap": "^1.3.2",
		"jsdoc": "^3.6.2",
		"jsdom": "15.1.0",
		"jsdom-global": "3.0.2",
		"mocha": "^6.1.4",
		"uglifyjs-webpack-plugin": "^2.1.3",
		"webpack": "^4.31.0",
		"webpack-cli": "^3.3.2",
		"webpack-node-externals": "^1.7.2",
		"yargs": "^13.2.4"
	},
	"dependencies": {
		"line-by-line": "^0.1.6"
	}
}
