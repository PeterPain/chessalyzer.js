{
	"root": true,
	"env": {
		"node": true
	},
	"extends": ["eslint-config-airbnb-base"],
	"rules": {
		"indent": ["error", "tab", { "SwitchCase": 1 }],
		"no-tabs": 0,
		"func-names": ["error", "never"],
		"comma-dangle": ["error", "never"],
		"operator-linebreak": 0,
		"arrow-parens": "off",
		"no-console": "off",
		"implicit-arrow-linebreak": "off",
		"max-classes-per-file": "off"
	}
}
